describe("Calcular sumaDeLosParesDel0Al(6)", function() {
  it("deberia retornar 12", function() {
    assert.equal(sumaDeLosParesDel0Al(6), 12)
  });
})
describe("Calcular sumaDeLosParesDel0Al(10)", function() {
  it("deberia retornar 30", function() {
    assert.equal(sumaDeLosParesDel0Al(10), 30)
  });
})


